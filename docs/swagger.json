{
    "swagger": "2.0",
    "info": {
        "contact": {}
    },
    "paths": {
        "/healthcheck": {
            "get": {
                "description": "Send a welcome message if the server is online",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "info"
                ],
                "summary": "Check if the server is online",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/httputil.Message"
                        }
                    }
                }
            }
        },
        "/status": {
            "get": {
                "description": "Return information about the server",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "info"
                ],
                "summary": "Status of the server",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/controllers.StatusResponse"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "controllers.StatusResponse": {
            "type": "object",
            "properties": {
                "database": {
                    "type": "string"
                },
                "redis": {
                    "type": "string"
                },
                "version": {
                    "type": "string"
                },
                "welcome": {
                    "type": "string"
                }
            }
        },
        "httputil.Message": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer",
                    "example": 200
                },
                "data": {},
                "message": {
                    "type": "string",
                    "example": "a server message"
                }
            }
        }
    }
